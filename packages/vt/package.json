{
  "name": "@mana-ssh/vt",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "clean": "git clean -xfd .",
    "clean:artifacts": "rm -rf dist",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --reporter=dot --silent",
    "test:watch": "vitest --reporter=default --silent=false",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "fast-check": "^3.18.0",
    "typescript": "^5.0.4",
    "vitest": "^3.2.4"
  }
}
