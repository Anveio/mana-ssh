{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "start": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "tsconfig.tsbuildinfo"]
    },
    "lint": {
      "dependsOn": ["^topo"],
      "outputs": []
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "persistent": true,
      "cache": false
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^topo"],
      "outputs": []
    }
  }
}
